<push an existing repository>
git remote add origin https://github.com/humphreyhung/flow.git
git push -u origin master
git push --all
git push --tags

<pull from repositiry>
git pull --rebase origin master (no merge line)

<usually use commad>
git init
git add
git rm
git mv
git commit -m
git status
git log  --oneline
git reset (git rm --cached c.txt)
git checkout branch
git checkout -b branch (make branch and switch)
git reset --hard SHA1cod(6 digit)
git reset --hard ORIG_HEAD
git branch
git branch master (generte branch from master)
git branch -a
git branch -d
git merge
git tag
git tag -a -m
git stash
git stash -u (save also WD area)
git stash list
git stash pop
git diff (compare working with index, check before git add)
git diff HEAD (compare working with HEAD commit)
git diff --cached HEAD (compare index with HEAD, check beofre git commit)
git diff HEAD^ HEAD (compare previous HEAD with HEAD, 2 commit difference)
git show v1.0
git clone
git remote

git log -p
git log -U1 --word-diff

git commit --amend
git reset HEAD <file>

git remote -v
git fetch orgin (no merge)
git remote show orgin

git config --global alias.<cmd> <command> 
